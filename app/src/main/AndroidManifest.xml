        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".contohlain"></activity>
        <activity android:name=".relativelatout" />
        <activity
            android:name=".taglayoutweight"
            android:label="Halaman 4" />
        <activity
